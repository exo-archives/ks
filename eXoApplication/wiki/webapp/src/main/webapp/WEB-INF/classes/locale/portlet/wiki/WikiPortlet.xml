<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <javax>
    <portlet>
      <title>Wiki</title>
    </portlet>
  </javax>  
  <!--
   #org.exoplatform.wiki.webui.control.action.DeletePageActionComponent
  -->
  <DeletePageAction>
    <msg>
    <Warning>Can not delete wiki home page!</Warning>
    </msg>
    <confirm>
    <Message>Are you sure to delete this page?</Message>
    </confirm>
  </DeletePageAction>
  <!--
   #org.exoplatform.calendar.webui.popup.UIAdvancedSearchForm
  -->
  <UIWikiAdvanceSearchForm>
    <label>
      <space>Space</space>
    </label>
    <msg>
      <there-is-no-search-result>There is no search result...!
      </there-is-no-search-result>
    </msg>
    <action>
      <Search>Search</Search>
    </action>
  </UIWikiAdvanceSearchForm>  
  <!--
   #org.exoplatform.wiki.webui.popup.UIWikiEditPreferencesForm
  -->
  <UIWikiEditPreferencesForm>
    <label>
      <DefaultSyntax>Default syntax</DefaultSyntax>
      <AllowChoseOthers>Allow User to chose others syntaxes</AllowChoseOthers>
    </label>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIWikiEditPreferencesForm>
  <!--
   #org.exoplatform.wiki.webui.popup.UIWikiMovePageForm
  -->
  <UIWikiMovePageForm>
    <label>
      <CurrentLocation>Current Location</CurrentLocation>
      <NewLocation>New Location</NewLocation>
    </label>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
    <msg>
      <can-not-move>Can not move</can-not-move>
      <same-name-in-new-location-space>Same name in new location space</same-name-in-new-location-space>
      <new-location-is-empty>New location is empty</new-location-is-empty>
    </msg>
  </UIWikiMovePageForm>
  <!--
   #org.exoplatform.webui.core.UIPopupWindow
  -->
  <UIWikiPopupWindow>
    <title>
      <UIWikiEditPreferencesForm>Edit Preferences</UIWikiEditPreferencesForm>
      <UIWikiMovePageForm>Move Page</UIWikiMovePageForm>
    </title>
  </UIWikiPopupWindow>
</bundle>
