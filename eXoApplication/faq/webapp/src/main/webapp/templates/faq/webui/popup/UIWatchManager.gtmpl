<div class="UIWatchManager $uicomponent.id" style="padding: 8px;">
		<% uiform.begin() %>
		<div class="UIForm">
 			<div class="UIHorizontalLayout">
 				<div class="UIPermissionManager">
 					<div class="UIPermissionInfo">
 						<table class="UIGrid" cellspacing="0" borderspacing="0">
 							<thead>
 								<tr>
 								<th><%= _ctx.appRes("UIWatchManager.label.email"); %></th>
 								<th><%= _ctx.appRes("UIWatchManager.label.action"); %></th>
 								</tr>
 							</thead>
 							<tbody>
 							<% 
 							String categoryId = uicomponent.getCategoryID() ;
 							List emails = uicomponent.getListEmail() ;
 							String linkCurrent = uicomponent.event("Link", categoryId)
 							if(emails.size() > 0){
 								for(String email : emails ) {
 									email = email.substring(0,email.length())
 									String linkEdit = uicomponent.event("EditEmail", email);
 									String linkDelete = uicomponent.event("DeleteEmail", "$uicomponent.id", email);
 							 %>
 								<tr>
 									<% if(email.length() > 90) {
 											String title = email ;
 											email = uicomponent.getSubString(email,90)
 									 %>
 										<td class="text" align="left"><a title="$title">$email</a></td>
 									<% } else { %>
 										<td class="text" align="left">$email</td>
 									<% } %>
 									<td style="width: 90px;" align="center">
 										<a href="$linkCurrent" title="<%= _ctx.appRes("UIWatchManager.title.link"); %>" style="float:left;">
 											<div class="LinkWatchIcon" ></div>
 										</a>
 										<a href="$linkEdit" title="<%= _ctx.appRes("UIWatchManager.title.edit-watch"); %>" style="float:left;">
 											<div class="EditWatchIcon" ></div>
 										</a>
 										<a href="$linkDelete" title="<%= _ctx.appRes("UIWatchManager.title.delete-watch"); %>" style="float:left;">
 											<div class="DeleteWatchIcon" ></div>
 										</a>
 									</td>
 								</tr>
 								<% 
 									}
 								} else {
 									   %>
 										 <tr class="EvenRow">
												<td class="Tdbox" colspan="10" align="center"><div class="" ><%= _ctx.appRes("UIWatchManager.title.no-matches"); %> </div></td>
			  						</tr>
								<% } %>
 							</tbody>
 						</table>
 						
 					</div>
 				</div>
 			</div>
 		</div>
		<div class="UIAction">
    	<table class="ActionContainer" align="center">
			  <tr>
				  <td align="center">
		    		<a href="<%=uicomponent.event("Cancel");%>" class="ActionButton LightBlueStyle">
			  	  	<div class="ButtonLeft">
			    	  	<div class="ButtonRight">
			      	  	<div class="ButtonMiddle"><%=_ctx.appRes(uicomponent.getName() + ".action.Cancel");%></div>
			        	</div>
			      	</div>
		    		</a>
	      	</td>
				</tr>
 	  </table>
	</div>
		<%uiform.end()%>
</div>